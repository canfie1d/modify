}

.page-wrapper {
  position: relative;
  height: auto;
  width: 100%;
  margin-top: calc(100vh - 100px);
  box-sizing: content-box;
  background: $white;

  .page__content {
    max-width: 1260px;
    margin: auto;
  }
}

.content-flex {
  display: flex;
  flex-direction: column;
  align-items: center;
  padding: 200px 20px;
}
